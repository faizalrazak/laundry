<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.itrain.laundry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".BookingDetailActivity" />
        <activity android:name=".LaundryDetailActvity" />
        <activity android:name=".LocationActivity" />
        <activity
            android:name=".PromoActivity"
            android:label="@string/title_activity_promo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PickupDeliveryActivity" />
        <activity android:name=".MyOrderActivity"></activity>
    </application>

</manifest>